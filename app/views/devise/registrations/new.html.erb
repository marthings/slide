<h2>Sign up</h2>

<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>

  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation, label: false %>
  <%= f.button :submit, class: "btn-success" %>
  
<% end %>

<%= render "devise/shared/links" %>
